{
    "name": "bouksou/uploader",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/bouksou/uploader"
      }
    ],
    "type": "cakephp-plugin",
    "description": "CakePHP 3 uploader",
    "keywords": ["cakephp", "uploader", "imagine"],
    "homepage": "https://github.com/bouksou/uploader",
    "authors": [
        {
            "name": "Bouksou",
            "homepage": "http://www.bouksou.com",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/bouksou/uploader/issues",
        "source": "https://github.com/bouksou/uploader"
    },
    "require": {
        "cakephp/cakephp": "3.0.*-dev",
        "cakephp/plugin-installer": "*",
        "imagine/imagine": "dev-master"
    },
    "require-dev": {
		"cakephp/cakephp-codesniffer": "2.0.*-dev",
		"phpunit/phpunit": "4.1.*"
	},
	"autoload": {
		"psr-4": {
			"DebugKit\\": "src",
			"DebugKit\\Test\\Fixture\\": "tests\\Fixture"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"DebugKit\\Test\\": "tests"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.2.x-dev"
		}
	}
}
